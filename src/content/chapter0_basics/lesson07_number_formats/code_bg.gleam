import gleam/io

pub fn main() {
  // Долни черти
  io.debug(1_000_000)
  io.debug(10_000.01)

  // Литерали за цели числа в двоичен, осмичен и шестнадесетичен формат
  io.debug(0b00001111)
  io.debug(0o17)
  io.debug(0xF)

  // Литерали за числа с плаваща запетая в научна нотация
  io.debug(7.0e7)
  io.debug(3.0e-4)
}
